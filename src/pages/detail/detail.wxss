@import "/wxParse/wxParse.wxss";
.detail_box {
  padding: 0 10px;
}
.detail_title {
  display: block;
  font-size: 60rpx;
  color: #333333;
  line-height: 30px;
  margin-top: 28px;
}
.detail_time {
  display: block;
  font-size: 28rpx;
  color: #828282;
  margin-top: 18px;
  margin-bottom: 30px;
}
.wxParse-img {
  max-width: 100%;
  max-height: 100%;
  display: block;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  -webkit-box-shadow: 0 0 10px #555;
  box-shadow: 0 0 10px #555;
  margin: 20px 0 30px 0;
}
